{
  "name": "generator-siigo",
  "version": "2.1.0",
  "description": "Generator archetype siigo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"{generators,test,utils}/**/*.js\"",
    "release": "standard-version --skip.commit --skip.tag --header \"# Changelog\n\nAll notable changes to this project.\n\"",
    "test": "mocha --timeout 30000",
    "test:debug": "mocha --inspect-brk",
    "coverage": "nyc --reporter lcov --reporter text --report-dir .nyc_output npm run test",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/SiigoDevOps/Siigo/_git/Siigo.Cli"
  },
  "author": "Arquitectura",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://dev.azure.com/SiigoDevOps/Siigo/_git/Siigo.Cli"
  },
  "homepage": "https://dev.azure.com/SiigoDevOps/Siigo/_git/Siigo.Cli#readme",
  "keywords": [
    "yeoman-generator",
    "angular",
    "component",
    "javascript",
    "builder",
    "scaffold",
    "generator",
    "s9s"
  ],
  "dependencies": {
    "@azure/storage-blob": "^12.6.0",
    "@nodesiigo/siigosay": "^1.0.0",
    "chalk": "^3.0.0",
    "colors": "^1.4.0",
    "decamelize": "^3.2.0",
    "fuzzy": "^0.1.3",
    "gulp-rename": "1.2.2",
    "inquirer": "^8.1.1",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "json-colorizer": "^2.2.2",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "prompt": "^1.1.0",
    "replace-in-file": "^4.2.0",
    "shelljs": "^0.8.4",
    "update-notifier": "^5.1.0",
    "uppercamelcase": "^3.0.0",
    "yeoman-generator": "^5.3.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^5.1.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "standard-version": "^9.3.0",
    "svg-term-cli": "^2.1.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^3.4.1",
    "yeoman-test": "^6.1.0"
  },
  "files": [
    "generators",
    "utils",
    "tribes"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
