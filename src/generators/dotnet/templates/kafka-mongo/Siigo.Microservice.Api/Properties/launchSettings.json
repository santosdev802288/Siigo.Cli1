{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:58169",
      "sslPort": 44320
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "<%= config.projectPrefix %>.<%= config.nameCapitalize %>": {
      "commandName": "Project",
      "launchBrowser": false,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "local"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/product/hello",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:443;http://+:80",
        "ASPNETCORE_HTTPS_PORT": "44321"
      },
      "httpPort": 58171,
      "useSSL": true,
      "sslPort": 44321
    }
  }
}