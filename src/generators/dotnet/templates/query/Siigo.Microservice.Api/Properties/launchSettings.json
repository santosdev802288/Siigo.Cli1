{
    "iisSettings": {
        "windowsAuthentication": false,
        "anonymousAuthentication": true,
        "iisExpress": {
            "applicationUrl": "http://localhost:58169",
            "sslPort": 44320
        }
    },
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "profiles": {
        "IIS Express": {
            "commandName": "IISExpress",
            "launchBrowser": true,
            "launchUrl": "docs",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Local"
            }
        },
        "Siigo.<%= config.nameCapitalize %>": {
            "commandName": "Project",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Local"
            },
            "applicationUrl": "http://localhost:5000/api/docs"
        },
        "Docker": {
            "commandName": "Docker",
            "launchBrowser": true,
            "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/docs",
            "environmentVariables": {
                "ASPNETCORE_URLS": "https://+:443;http://+:5000",
                "ASPNETCORE_HTTPS_PORT": "44321"
            },
            "httpPort": 5000,
            "useSSL": false,
            "sslPort": 44321
        }
    }
}