{
    "AllowedHosts": "*",
    "Kestrel": {
        "Limits": {
            "MaxConcurrentConnections": 2000,
            "MaxConcurrentUpgradedConnections": 2000,
            "MaxRequestBodySize": 10240,
            "MaxRequestHeaderCount": 500
        },
        "EndPoints": {
            "Http": {
                "Url": "http://*:5000"
            },
            "Http2": {
              "Url": "http://*:5002",
              "Protocols": "Http2"
            }
        }
    }  
}