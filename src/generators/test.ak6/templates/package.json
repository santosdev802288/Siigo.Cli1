  "license": "UNLICENSED",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "monitors": "docker-compose up -d influxdb grafana",
