{
  "name": "ak-6",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:go-automate/k6-typescript-framework.git",
  "author": "Simon Stratton <simon.stratton@safebear.co.uk>",
  "license": "MIT",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "monitors": "docker-compose up -d influxdb grafana",
    "build": "docker-compose pull && webpack",
    "test": "yarn build && docker-compose run --rm k6 run dist/test.js",
    "seed": "yarn build && docker-compose run --rm k6 run dist/seed.js",
    "seed-distributed": "yarn build && docker-compose run --rm k6 run dist/seed-distributed.js -e ENV=qa"
  },
  "dependencies": {
    "@types/k6": "^0.36.0",
    "@types/node": "^13.7.1",
    "cli-select": "^1.1.2",
    "fs": "0.0.1-security",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
