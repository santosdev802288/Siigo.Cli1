        "node": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "ignorePatterns": ["**/templates/*.js"],
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }]
    }
}
